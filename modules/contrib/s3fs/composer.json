{
    "name": "drupal/s3fs",
    "description": "Adds an Amazon Simple Storage Service-based remote file system to Drupal.",
    "type": "drupal-module",
    "homepage": "https://www.drupal.org/project/s3fs",
    "authors": [
        {
            "name": "Abhishek Anand",
            "email": "fly2abhishek@gmail.com"
        },
        {
            "name": "Juanen Bernal",
            "homepage": "https://www.drupal.org/u/jansete"
        }
    ],
    "support": {
        "issues": "https://www.drupal.org/project/issues/s3fs"
    },
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "aws/aws-sdk-php": "^3.18"
    },
    "require-dev": {
        "drush/drush": "^10 || ^11 || ^12 || ^13",
        "drupal/key": "^1",
        "mglaman/phpstan-drupal": "^1.1",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "suggest": {
        "doctrine/cache": "~1.4"
    }
}
